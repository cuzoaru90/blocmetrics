<div class = "container center-text">

<h1>Users</h1>

<% @users.each do |user| %>
<p>

  <div class = 'row'>

    <div class="col-md-2"> 
      <%= link_to user.name, user %>
    </div>

     <div class="col-md-2"> 
      <%= user.email %>
    </div>

  </div>

</p>

<% end %>

</div>

<%= will_paginate @users %>